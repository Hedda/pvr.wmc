msgctxt "#30000"
msgid "Server Hostname or IP"
msgstr "Име хоста или IP адреса сервера"

msgctxt "#30001"
msgid "Keep IP address to 127.0.0.1, if server is running on this same system"
msgstr "Задржите IP адресу 127.0.0.1, ако је сервер покренут на овом систему"

msgctxt "#30002"
msgid "Port Number"
msgstr "Број порта"

msgctxt "#30004"
msgid "No video data found on this channel"
msgstr "На овом каналу не постоје видео подаци"

msgctxt "#30005"
msgid "Tuner is needed for recording"
msgstr "Потребан вам је пријемник за снимање"

msgctxt "#30006"
msgid "Recording delete failed, file in use or write protected"
msgstr "Није успело брисање снимка, датотека је у употреби или за заштићена против снимања"

msgctxt "#30007"
msgid "Recording delete failed, recorded file doesn't exist"
msgstr "Није успело брисање снимка, не постоји датотека снимка"

msgctxt "#30008"
msgid "Recording file not found"
msgstr "Датотека снимка није пронађена"

msgctxt "#30102"
msgid "Record the series"
msgstr "Сними целу серију"

msgctxt "#30103"
msgid "Show type:"
msgstr "Прикажи врсту:"

