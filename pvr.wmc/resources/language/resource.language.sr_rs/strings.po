msgid "New and rerun"
msgstr "Нове и репризе"

msgctxt "#30105"
msgid "New only"
msgstr "Само нове"

msgctxt "#30106"
msgid "Live only"
msgstr "Само емитовање уживо"

msgctxt "#30122"
msgid "Delete the entire series"
msgstr "Обриши целу серију"
